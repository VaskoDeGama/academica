{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "ignorePatterns": ["config/*.js"],
    "rules": {
        "semi": ["error", "never"],
        "node/no-path-concat": ["off"]
    }
}
